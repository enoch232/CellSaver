    <section id="subscribe" class="download bg-primary text-center">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <h2 class="section-heading">Discover what all the buzz is about!</h2>
                    <p>Sign up to get notified!</p>
                    <%= form_for @user, url: users_path, method: :post, class: "form-inline" do |f|%>
                      <div class="input-group">
                        <%= f.text_field :email, class: "form-control", placeholder: "Email Address", style: "font-size: 15px;height:50px", size: 50 %>
                        <div class="input-group-btn">
                          <%= f.submit :Subscribe, class: "btn btn-outline btn-xl" %>
                        </div>
                      </div>
                    <% end %>
                </div>
            </div>
        </div>
    </section>

